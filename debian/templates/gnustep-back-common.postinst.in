#!/bin/sh
# postinst script for gnustep-back
#
# see: dh_installdeb(1)

set -e

# Cleanup all stuff that was used by the art backend (#885786).
# Delete this file after bookworm is released.

FONTSDIR=/var/lib/GNUstep

case "$1" in
    configure)
	if [ -d $FONTSDIR ]; then
	    rm -rf $FONTSDIR
	fi
        rm -f /var/log/gnustep-back-common.log
    ;;

    abort-upgrade|abort-remove|abort-deconfigure)
    ;;

    *)
        echo "postinst called with unknown argument \`$1'" >&2
        exit 1
    ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
